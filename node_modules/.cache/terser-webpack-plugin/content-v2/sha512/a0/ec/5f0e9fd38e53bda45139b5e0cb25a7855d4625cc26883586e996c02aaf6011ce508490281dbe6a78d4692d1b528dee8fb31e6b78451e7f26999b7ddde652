state && this.props.location.state.id,\\r\\n      detailAvail: 'Loading....'\\r\\n    };\\r\\n  }\\r\\n  componentDidMount() {\\r\\n    typeof this.state.id === 'undefined'\\r\\n      ? this.props.history.goBack()\\r\\n      : this.detailAvail(this.state);\\r\\n  }\\r\\n  detailAvail = async () => {\\r\\n    this.setState({ detailAvail: await fetchDetailAvail(this.state) });\\r\\n  };\\r\\n  change = async e => {\\r\\n    let { name, value } = e.target;\\r\\n    this.setState({ [name]: value });\\r\\n  };\\r\\n  submit = async () => {\\r\\n    if (this.state.num !== '') {\\r\\n      let a1 = window.confirm('R U Sure.....');\\r\\n      a1 && (await this.setState({ detailAvail: 'Adding...' }));\\r\\n      a1 &&\\r\\n        (await this.setState({\\r\\n          detailAvail: await add(this.state)\\r\\n        }));\\r\\n    } else {\\r\\n      alert('Enter Something.....');\\r\\n    }\\r\\n  };\\r\\n  remove = async e => {\\r\\n    let { id } = e.target;\\r\\n    let a = window.confirm('R U Sure...?');\\r\\n    a && (await this.setState({ detailAvail: 'Deleting...' }));\\r\\n    a &&\\r\\n      (await this.setState({\\r\\n        detailAvail: await remove(this.state, id)\\r\\n      }));\\r\\n  };\\r\\n  render() {\\r\\n    return (\\r\\n      <div>\\r\\n        <nav className='navbar-fixed'>\\r\\n          {console.log(this.props.match.params.id, this.props.location.state)}\\r\\n          <div className='nav-wrapper'>\\r\\n            <div className='brand-logo center'>\\r\\n              {this.state.detailAvail && this.state.detailAvail.Name}\\r\\n            </div>\\r\\n          </div>\\r\\n        </nav>\\r\\n        <div className='row center-align'>\\r\\n          <div className='col s12 m12 l6'>\\r\\n            <table className='highlight centered'>\\r\\n              <tbody>\\r\\n                {this.state.detailAvail &&\\r\\n                this.state.detailAvail === 'Loading....' ? (\\r\\n                  <DeletingGif text={this.state.detailAvail} />\\r\\n 