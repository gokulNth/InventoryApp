';\\r\\nimport loading from '../../Gif/Loading/loading.gif';\\r\\nimport Chip from './Chip';\\r\\n\\r\\nconst Homepage = props => {\\r\\n  return (\\r\\n    <div>\\r\\n      {props.state.isloading === '' ? (\\r\\n        <div className='row'>\\r\\n          <div\\r\\n            className='col s12 m6 l8'\\r\\n            style={{\\r\\n              overflow: 'auto',\\r\\n              height: 560,\\r\\n              marginBottom: window.innerWidth < 500 && '40%'\\r\\n            }}\\r\\n          >\\r\\n            <div className='row'>\\r\\n              <Navbar empty={props.empty} search={props.search} />\\r\\n              <Chip list={props.state.list} add={props.add} />\\r\\n              {props.state.products.map(i => {\\r\\n                return <ItemList item={i} key={i.Name} add={props.add} />;\\r\\n              })}\\r\\n            </div>\\r\\n          </div>\\r\\n          <div\\r\\n            className='col s12 m6 l4 z-depth-5 '\\r\\n            style={{\\r\\n              overflow: 'auto',\\r\\n              height: 560,\\r\\n              marginTop: window.innerWidth < 500 ? '-40%' : 5,\\r\\n              marginLeft: window.innerWidth > 500 && -5\\r\\n            }}\\r\\n          >\\r\\n            <Footer\\r\\n              selected_items={props.state.selected_items}\\r\\n              bill={props.state.totPrice}\\r\\n              all={props.state.items}\\r\\n              empty={props.empty}\\r\\n            />\\r\\n            <div style={{ overflow: 'auto', height: 400 }}>\\r\\n              {props.state.items.length !== 0 ? (\\r\\n                props.state.items.map((i, index) => {\\r\\n                  return (\\r\\n                    <SelectedItemList\\r\\n                      item={props.state.selected_items[i]}\\r\\n                      name={i}\\r\\n                      key={index}\\r\\n                      delt={props.delete}\\r\\n                      one={props.one}\\r\\n                    />\\r\\n                  );\\r\\n  