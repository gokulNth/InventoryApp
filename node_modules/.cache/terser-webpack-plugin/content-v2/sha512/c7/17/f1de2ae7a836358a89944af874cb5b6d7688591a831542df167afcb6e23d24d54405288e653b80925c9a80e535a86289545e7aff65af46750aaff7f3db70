\\r\\n            {props.item.Name}\\r\\n          </span>\\r\\n          <div className='right' onClick={props.add} id={props.item.Name}>\\r\\n            Price : {props.item.Price}\\r\\n          </div>\\r\\n        </div>\\r\\n      </div>\\r\\n    </div>\\r\\n  );\\r\\n};\\r\\nexport default ItemList;\\r\\n\",\"import React from 'react';\\r\\n\\r\\nconst Input = props => {\\r\\n  return (\\r\\n    <div className='row'>\\r\\n      <div className='col s3 m3 l3' />\\r\\n      <div className='col s6 m6 l6'>\\r\\n        <input\\r\\n          type='number'\\r\\n          id={props.id}\\r\\n          value={props.req}\\r\\n          onChange={props.one}\\r\\n          style={{ height: 20, textAlign: 'center', marginLeft: 15 }}\\r\\n        />\\r\\n      </div>\\r\\n      <div className='col s3 m3 l3' />\\r\\n    </div>\\r\\n  );\\r\\n};\\r\\n\\r\\nexport default Input;\\r\\n\",\"import React from 'react';\\r\\nimport Input from './Input';\\r\\n\\r\\nconst Selected_ItemList = props => {\\r\\n  return (\\r\\n    <div\\r\\n      className='card red lighten-3'\\r\\n      style={{ height: '5rem', borderRadius: 20 }}\\r\\n    >\\r\\n      <div className='card-content red-text'>\\r\\n        <div className='row'>\\r\\n          <div className='col s6 m3 l6'>\\r\\n            <span className='white-text'>{props.name}</span>\\r\\n          </div>\\r\\n          <div className='col s6 m9 l6'>\\r\\n            <div className='row'>\\r\\n              <div className='col s10 m10 l10'>\\r\\n                <Input id={props.name} req={props.item.req} one={props.one} />\\r\\n              </div>\\r\\n              <div className='col s2 m2 l2'>\\r\\n                <i\\r\\n                  className='material-icons'\\r\\n                  onClick={props.delt}\\r\\n                  id={props.name}\\r\\n                  style={{\\r\\n                    fontSize: '2rem',\\r\\n                    left: '10%',\\r\\n                    cursor: 'pointer'\\r\\n                  }}\\r\\n