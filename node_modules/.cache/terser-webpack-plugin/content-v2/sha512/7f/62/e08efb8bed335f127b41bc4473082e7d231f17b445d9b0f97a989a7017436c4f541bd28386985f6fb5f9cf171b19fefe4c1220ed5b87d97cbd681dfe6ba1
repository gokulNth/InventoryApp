ed_items}\\r\\n              bill={props.state.totPrice}\\r\\n              all={props.state.items}\\r\\n              empty={props.empty}\\r\\n            />\\r\\n            <div style={{ overflow: 'auto', height: 400 }}>\\r\\n              {props.state.items.length !== 0 ? (\\r\\n                props.state.items.map((i, index) => {\\r\\n                  return (\\r\\n                    <SelectedItemList\\r\\n                      item={props.state.selected_items[i]}\\r\\n                      name={i}\\r\\n                      key={index}\\r\\n                      delt={props.delete}\\r\\n                      one={props.one}\\r\\n                    />\\r\\n                  );\\r\\n                })\\r\\n              ) : (\\r\\n                <h5 className='center'>--NONE--</h5>\\r\\n              )}\\r\\n            </div>\\r\\n          </div>\\r\\n        </div>\\r\\n      ) : (\\r\\n        (props.state.isloading === 'Loading...' || 'serverError') && (\\r\\n          <React.Fragment>\\r\\n            <div style={{ margin: '0 auto', width: '50%' }}>\\r\\n              <img\\r\\n                src='https://cdn.dribbble.com/users/563824/screenshots/3633228/untitled-5.gif'\\r\\n                alt='Loading...'\\r\\n                style={{ margin: '0 auto', width: '99%' }}\\r\\n              />\\r\\n            </div>\\r\\n          </React.Fragment>\\r\\n        )\\r\\n      )}\\r\\n    </div>\\r\\n  );\\r\\n};\\r\\n\\r\\nexport default Homepage;\\r\\n\",\"import React from 'react';\\r\\nimport { fetch } from '../../Fetches';\\r\\nimport Homepage from '../Layouts/Homepage';\\r\\n\\r\\nclass Dashboard extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n      products: [],\\r\\n      selected_items: [],\\r\\n      items: [],\\r\\n      totPrice: 0,\\r\\n      isloading: 'Loading...',\\r\\n      list: []\\r\\n    };\\r\\n  }\\r\\n  componentDidMount() {\\r\\n    this.props.location.state &&\\r\\n     